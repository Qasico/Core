{
    "name": "Qasico/Core",
    "type": "library",
    "description": "Qasico Application Platform",
    "authors": [
        {
            "name": "PT. Qasico Teknologi Indonesia",
            "email": "admin@qasico.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-curl": "*",
        "predis/predis": "~1.0",
        "vlucas/phpdotenv": "~2.0",
        "robmorgan/phinx": "0.5.*",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/framework": "5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*",
        "fzaninotto/faker": "~1.4",
        "squizlabs/php_codesniffer": "~2.3",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "symfony/css-selector": "2.8.*|3.0.*",
        "hot/phpunit-runner": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Qasico\\": "src"
        },
        "files": [
            "src/Support/Helpers/app.php",
            "src/Support/Helpers/date.php",
            "src/Support/Helpers/utils.php",
            "src/Support/Helpers/template.php",
            "src/Support/Helpers/encryption.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
